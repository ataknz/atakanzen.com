{
  "presets": ["next/babel"],
  "plugins": [
    [
      "prismjs",
      {
        "languages": ["javascript", "css", "markup", "python", "go"],
        "plugins": [
          "line-numbers",
          "autolinker",
          "copy-to-clipboard",
          "autoloader",
          "show-language",
          "match-braces"
        ],
        "theme": "tomorrow",
        "css": true
      }
    ]
  ]
}
